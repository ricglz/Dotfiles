alias b='brew'
alias bi='brew install'
alias bu='brew uninstall'
alias bupgr='brew upgrade && brew cask upgrade && brew cleanup && brew doctor'
alias cask='brew cask'
alias caski='brew cask install'
alias casku='brew cask uninstall'

alias dcb='docker-compose build'
alias dcdn='docker-compose down'
alias dcr='docker-compose run --rm'

alias dcrgm='docker-compose run --rm app rails generate migration'
alias dcrra='docker-compose run --rm app rails'
alias dcrspec='docker-compose run --rm app rspec'

alias dcup='docker-compose up'
alias dcla='docker stop $(docker container ls -a -q) && docker system prune -a -f --volumes'

alias ins_net="nmap -sP $(ipconfig getifaddr en1)/24"

alias gi='gem install'
alias gu='gem uninstall'
alias gcc_lex='gcc lex.yy.c -o program.out -ll'

alias npmglist='npm list -g --depth 0'
alias nupgr='npm update -g'

alias python='/usr/local/bin/python2.7'

alias rupgr='gem update && gem cleanup'
