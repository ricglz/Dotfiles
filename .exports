# Configure PATH
export PATH="/usr/local/sbin:$HOME/.npm-packages/bin:/usr/local/opt/ruby/bin"
export PATH="$PATH:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"
export PATH="$PATH:$HOME/.cargo/bin:/usr/local/opt/python/libexec/bin"
export PATH="$PATH:$HOME/Library/Python/3.8/bin:$HOME/extra/bin"

# Other exports
export EDITOR=nvim
export LANG=en_US.UTF-8
export DEFAULT_USER=`whoami` # Removes user name and dispositve name in terminal
export FZF_DEFAULT_COMMAND='fd --type file'
export FZF_CTRL_T_COMMAND="$FZF_DEFAULT_COMMAND"
export PGHOST=localhost
export PGPORT=5432
